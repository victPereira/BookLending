@model IEnumerable<LendingModel>


<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Receiver</th>
            <th scope="col">Supplier</th>
            <th scope="col">Book</th>
            <th scope="col">Date Att</th>
        </tr>
    </thead>
    <tbody> 


        @if(Model != null)
        {
            @foreach(var lending in Model)
            {               
                <tr>
                    <th scope="row">@lending.Receiver</th>
                    <td>@lending.Supplier</td>
                    <td>@lending.BookLend</td>
                    <td>@lending.DataEndAtt</td>
                </tr>
            }
        }
    </tbody>
</table>